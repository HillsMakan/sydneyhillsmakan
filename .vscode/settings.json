{
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "astro",
      "typescript",
      "typescriptreact"
    ],
    "prettier.documentSelectors": ["**/*.astro"],
    "[astro]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    // The number of spaces a tab is equal to
    "editor.tabSize": 2,
    // insert spaces when pressing Tab
    "editor.insertSpaces": true,
  }